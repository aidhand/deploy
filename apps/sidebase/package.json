{
  "name": "sidebase",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --port 3001",
    "generate": "nuxt generate",
    "fix": "bun run fix:oxlint && bun run fix:eslint",
    "fix:oxlint": "bun run lint:oxlint -- --fix",
    "fix:eslint": "bun run lint:eslint -- --fix",
    "lint": "bun run lint:oxlint && bun run lint:eslint",
    "lint:eslint": "eslint --max-warnings 0 .",
    "lint:oxlint": "oxlint -D correctness -D suspicious -D perf",
    "start": "bun run .output/server/index.mjs",
    "typecheck": "nuxt typecheck",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "peerDependencies": {
    "next-auth": "4.21.1"
  },
  "dependencies": {
    "@nuxt/ui": "^3.0.0-alpha.12",
    "@prisma/client": "^6.3.0",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "@vueuse/nuxt": "12.5.0",
    "nuxt": "^3.15.4",
    "superjson": "^2.2.1",
    "trpc-nuxt": "^0.10.21",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.26.0",
    "@nuxt/fonts": "^0.10.3",
    "@nuxtjs/fontaine": "0.4.4",
    "@sidebase/nuxt-auth": "^0.9.4",
    "eslint": "^8.57.0",
    "oxlint": "^0.7.2",
    "prisma": "^6.3.0",
    "typescript": "^5.6.3",
    "unocss": "^65.4.3",
    "vite-node": "^2.1.1",
    "vue-tsc": "^2.1.6"
  }
}
